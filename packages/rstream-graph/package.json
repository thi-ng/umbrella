{
    "name": "@thi.ng/rstream-graph",
    "version": "3.2.81",
    "description": "Declarative dataflow graph construction for @thi.ng/rstream",
    "type": "module",
    "module": "./index.js",
    "typings": "./index.d.ts",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/rstream-graph#readme",
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/postspectacular"
        },
        {
            "type": "patreon",
            "url": "https://patreon.com/thing_umbrella"
        }
    ],
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn clean && tsc --declaration",
        "build:check": "tsc --isolatedModules --noEmit",
        "test": "testament test",
        "clean": "rimraf *.js *.d.ts *.map doc nodes",
        "doc:readme": "ts-node -P ../../tools/tsconfig.json ../../tools/src/readme.ts",
        "doc": "typedoc --excludePrivate --out doc --theme ../../tools/doc/typedoc-theme src/index.ts",
        "doc:ae": "mkdir -p .ae/doc .ae/temp && node_modules/.bin/api-extractor run --local --verbose",
        "pub": "yarn build:release && yarn publish --access public"
    },
    "dependencies": {
        "@thi.ng/api": "^7.2.0",
        "@thi.ng/atom": "^4.1.42",
        "@thi.ng/checks": "^2.9.11",
        "@thi.ng/errors": "^1.3.4",
        "@thi.ng/paths": "^4.2.14",
        "@thi.ng/resolve-map": "^4.2.27",
        "@thi.ng/rstream": "^6.0.21",
        "@thi.ng/transducers": "^7.9.2"
    },
    "devDependencies": {
        "@thi.ng/testament": "^0.0.1"
    },
    "keywords": [
        "animation",
        "async",
        "compute",
        "dataflow",
        "datastructure",
        "graph",
        "reactive",
        "rstream",
        "stream",
        "topology",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "*.js",
        "*.d.ts",
        "nodes"
    ],
    "exports": {
        ".": {
            "import": "./index.js"
        },
        "./api": {
            "import": "./api.js"
        },
        "./graph": {
            "import": "./graph.js"
        },
        "./nodes/extract": {
            "import": "./nodes/extract.js"
        },
        "./nodes/math": {
            "import": "./nodes/math.js"
        }
    },
    "thi.ng": {
        "parent": "@thi.ng/rstream",
        "year": 2018
    }
}
