        },
        "./api/copy": {
            "import": "./api/copy.js"
        },
        "./api/deref": {
            "import": "./api/deref.js"
        },
        "./api/dissoc": {
            "import": "./api/dissoc.js"
        },
        "./api/empty": {
            "import": "./api/empty.js"
        },
        "./api/enable": {
            "import": "./api/enable.js"
        },
        "./api/equiv": {
