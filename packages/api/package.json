		"./mixin": {
			"default": "./mixin.js"
		},
		"./mixins/ienable": {
			"default": "./mixins/ienable.js"
		},
		"./mixins/igrid": {
			"default": "./mixins/igrid.js"
		},
		"./mixins/inotify": {
			"default": "./mixins/inotify.js"
		},
		"./mixins/iterable": {
			"default": "./mixins/iterable.js"
		},
