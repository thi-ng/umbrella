		},
		"./reset": {
			"default": "./reset.js"
		},
		"./select": {
			"default": "./select.js"
		},
		"./seq": {
			"default": "./seq.js"
		},
		"./set": {
			"default": "./set.js"
		},
		"./stack": {
			"default": "./stack.js"
		},
		"./tuple": {
			"default": "./tuple.js"
		},
		"./typedarray": {
