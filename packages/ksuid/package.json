{
	"name": "@thi.ng/ksuid",
	"version": "3.2.90",
	"description": "Configurable K-sortable unique IDs, ULIDs, binary & base-N encoded, 32/48/64bit time resolutions",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"bin": "bin/ksuid",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://thi.ng/ksuid",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		},
		{
			"type": "liberapay",
			"url": "https://liberapay.com/thi.ng"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"bench": "bun bench/index.ts",
		"build": "yarn build:esbuild && yarn build:decl",
		"build:decl": "tsc --declaration --emitDeclarationOnly",
		"build:esbuild": "esbuild --format=esm --platform=neutral --target=es2022 --tsconfig=tsconfig.json --outdir=. src/**/*.ts",
		"clean": "bun ../../tools/src/clean-package.ts",
		"doc": "typedoc --options ../../typedoc.json --out doc src/index.ts",
		"doc:readme": "bun ../../tools/src/module-stats.ts && bun ../../tools/src/readme.ts",
		"pub": "yarn npm publish --access public",
		"test": "bun test",
		"tool:tangle": "../../node_modules/.bin/tangle src/**/*.ts"
	},
	"dependencies": {
		"@thi.ng/base-n": "^2.7.42",
		"@thi.ng/errors": "^2.5.36",
		"@thi.ng/random": "^4.1.21",
		"@thi.ng/strings": "^3.9.16"
	},
	"devDependencies": {
		"esbuild": "^0.25.6",
		"typedoc": "^0.28.7",
		"typescript": "^5.8.3"
	},
	"keywords": [
		"32bit",
		"64bit",
		"base62",
		"bigint",
		"binary",
		"bytes",
		"cli",
		"crypto",
		"database",
		"dynamodb",
		"encode",
		"epoch",
		"generator",
		"id",
		"millisecond",
		"random",
		"sort",
		"timestamp",
		"tool",
		"typescript",
		"ulid",
		"uuid"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"./*.js",
		"./*.d.ts",
		"bin"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./aksuid": {
			"default": "./aksuid.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./cli": {
			"default": "./cli.js"
		},
		"./ksuid32": {
			"default": "./ksuid32.js"
		},
		"./ksuid64": {
			"default": "./ksuid64.js"
		},
		"./ulid": {
			"default": "./ulid.js"
		}
	},
	"thi.ng": {
		"related": [
			"base-n",
			"idgen",
			"random"
		],
		"status": "stable",
		"year": 2020
	}
}
