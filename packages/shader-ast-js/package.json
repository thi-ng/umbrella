{
	"name": "@thi.ng/shader-ast-js",
	"version": "1.2.7",
	"description": "Customizable JS codegen, compiler & runtime for @thi.ng/shader-ast",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://thi.ng/shader-ast-js",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		},
		{
			"type": "liberapay",
			"url": "https://liberapay.com/thi.ng"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn build:esbuild && yarn build:decl",
		"build:decl": "tsc --declaration --emitDeclarationOnly",
		"build:esbuild": "esbuild --format=esm --platform=neutral --target=es2022 --tsconfig=tsconfig.json --outdir=. src/**/*.ts",
		"clean": "bun ../../tools/src/clean-package.ts env",
		"doc": "typedoc --options ../../typedoc.json --out doc src/index.ts",
		"doc:readme": "bun ../../tools/src/module-stats.ts && bun ../../tools/src/readme.ts",
		"pub": "yarn npm publish --access public",
		"test": "bun test",
		"tool:tangle": "../../node_modules/.bin/tangle src/**/*.ts"
	},
	"dependencies": {
		"@thi.ng/api": "^8.11.29",
		"@thi.ng/checks": "^3.7.9",
		"@thi.ng/errors": "^2.5.35",
		"@thi.ng/math": "^5.11.29",
		"@thi.ng/matrices": "^3.0.7",
		"@thi.ng/pixel": "^7.5.1",
		"@thi.ng/shader-ast": "^1.1.7",
		"@thi.ng/vectors": "^8.3.1"
	},
	"devDependencies": {
		"esbuild": "^0.25.5",
		"typedoc": "^0.28.5",
		"typescript": "^5.8.3"
	},
	"keywords": [
		"2d",
		"3d",
		"4d",
		"ast",
		"canvas",
		"codegen",
		"compiler",
		"dsl",
		"emulator",
		"glsl",
		"headless",
		"pixel",
		"render",
		"rgb565",
		"runtime",
		"shader",
		"shader-ast",
		"texture",
		"transpiler",
		"typescript",
		"webgl"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"./*.js",
		"./*.d.ts",
		"env"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./env/bvec": {
			"default": "./env/bvec.js"
		},
		"./env/float": {
			"default": "./env/float.js"
		},
		"./env/int": {
			"default": "./env/int.js"
		},
		"./env/ivec2": {
			"default": "./env/ivec2.js"
		},
		"./env/ivec3": {
			"default": "./env/ivec3.js"
		},
		"./env/ivec4": {
			"default": "./env/ivec4.js"
		},
		"./env/mat2": {
			"default": "./env/mat2.js"
		},
		"./env/mat3": {
			"default": "./env/mat3.js"
		},
		"./env/mat4": {
			"default": "./env/mat4.js"
		},
		"./env/uint": {
			"default": "./env/uint.js"
		},
		"./env/uvec2": {
			"default": "./env/uvec2.js"
		},
		"./env/uvec3": {
			"default": "./env/uvec3.js"
		},
		"./env/uvec4": {
			"default": "./env/uvec4.js"
		},
		"./env/vec2": {
			"default": "./env/vec2.js"
		},
		"./env/vec3": {
			"default": "./env/vec3.js"
		},
		"./env/vec4": {
			"default": "./env/vec4.js"
		},
		"./env": {
			"default": "./env.js"
		},
		"./pool": {
			"default": "./pool.js"
		},
		"./runtime": {
			"default": "./runtime.js"
		},
		"./target": {
			"default": "./target.js"
		}
	},
	"thi.ng": {
		"alias": "js",
		"parent": "@thi.ng/shader-ast",
		"related": [
			"pixel",
			"shader-ast-glsl",
			"shader-ast-stdlib"
		],
		"year": 2019
	}
}
