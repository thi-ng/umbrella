{
	"name": "@thi.ng/shader-ast-js",
	"version": "0.7.51",
	"description": "Customizable JS codegen, compiler & runtime for @thi.ng/shader-ast",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/shader-ast-js#readme",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn clean && tsc --declaration",
		"clean": "rimraf --glob '*.js' '*.d.ts' '*.map' doc env",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
		"doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
		"doc:readme": "yarn doc:stats && tools:readme",
		"doc:stats": "tools:module-stats",
		"pub": "yarn npm publish --access public",
		"test": "testament test"
	},
	"dependencies": {
		"@thi.ng/api": "^8.7.4",
		"@thi.ng/checks": "^3.3.10",
		"@thi.ng/errors": "^2.2.13",
		"@thi.ng/math": "^5.4.5",
		"@thi.ng/matrices": "^2.1.49",
		"@thi.ng/pixel": "^4.1.9",
		"@thi.ng/shader-ast": "^0.12.45",
		"@thi.ng/vectors": "^7.6.8"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.34.4",
		"@thi.ng/testament": "^0.3.13",
		"rimraf": "^4.4.0",
		"tools": "workspace:^",
		"typedoc": "^0.23.26",
		"typescript": "^4.9.5"
	},
	"keywords": [
		"ast",
		"canvas",
		"codegen",
		"compiler",
		"dsl",
		"emulator",
		"glsl",
		"shader",
		"typescript",
		"webgl"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts",
		"env"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./env/bvec": {
			"default": "./env/bvec.js"
		},
		"./env/float": {
			"default": "./env/float.js"
		},
		"./env/int": {
			"default": "./env/int.js"
		},
		"./env/ivec2": {
			"default": "./env/ivec2.js"
		},
		"./env/ivec3": {
			"default": "./env/ivec3.js"
		},
		"./env/ivec4": {
			"default": "./env/ivec4.js"
		},
		"./env/mat2": {
			"default": "./env/mat2.js"
		},
		"./env/mat3": {
			"default": "./env/mat3.js"
		},
		"./env/mat4": {
			"default": "./env/mat4.js"
		},
		"./env/uint": {
			"default": "./env/uint.js"
		},
		"./env/uvec2": {
			"default": "./env/uvec2.js"
		},
		"./env/uvec3": {
			"default": "./env/uvec3.js"
		},
		"./env/uvec4": {
			"default": "./env/uvec4.js"
		},
		"./env/vec2": {
			"default": "./env/vec2.js"
		},
		"./env/vec3": {
			"default": "./env/vec3.js"
		},
		"./env/vec4": {
			"default": "./env/vec4.js"
		},
		"./env": {
			"default": "./env.js"
		},
		"./runtime": {
			"default": "./runtime.js"
		},
		"./target": {
			"default": "./target.js"
		}
	},
	"thi.ng": {
		"parent": "@thi.ng/shader-ast",
		"related": [
			"shader-ast-glsl",
			"shader-ast-stdlib"
		],
		"year": 2019
	}
}
