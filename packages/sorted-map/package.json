{
	"name": "@thi.ng/sorted-map",
	"version": "1.2.25",
	"description": "Skiplist-based sorted map & set implementation",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/thi-ng/umbrella.git",
		"directory": "packages/sorted-map"
	},
	"homepage": "https://thi.ng/sorted-map",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		},
		{
			"type": "liberapay",
			"url": "https://liberapay.com/thi.ng"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn build:esbuild && yarn build:decl",
		"build:decl": "tsc --declaration --emitDeclarationOnly",
		"build:esbuild": "esbuild --format=esm --platform=neutral --target=es2022 --tsconfig=tsconfig.json --outdir=. src/**/*.ts",
		"clean": "bun ../../tools/src/clean-package.ts",
		"doc": "typedoc --options ../../typedoc.json --out doc src/index.ts",
		"doc:readme": "bun ../../tools/src/module-stats.ts && bun ../../tools/src/readme.ts",
		"pub": "npm publish --access public",
		"test": "bun test",
		"tool:tangle": "../../node_modules/.bin/tangle src/**/*.ts"
	},
	"dependencies": {
		"@thi.ng/api": "^8.12.13",
		"@thi.ng/associative": "^7.1.25",
		"@thi.ng/checks": "^3.8.3",
		"@thi.ng/compare": "^2.4.39",
		"@thi.ng/random": "^4.1.38",
		"@thi.ng/transducers": "^9.6.22"
	},
	"devDependencies": {
		"esbuild": "^0.27.2",
		"typedoc": "^0.28.16",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"datastructure",
		"equality",
		"iterator",
		"keys",
		"map",
		"query",
		"set",
		"skiplist",
		"sort",
		"typescript"
	],
	"publishConfig": {
		"access": "public"
	},
	"browser": {
		"process": false,
		"setTimeout": false
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"./*.js",
		"./*.d.ts"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./sorted-map": {
			"default": "./sorted-map.js"
		},
		"./sorted-set": {
			"default": "./sorted-set.js"
		}
	},
	"thi.ng": {
		"related": [
			"associative"
		],
		"year": 2018
	}
}
