{
    "name": "@thi.ng/pointfree",
    "version": "0.8.13",
    "description": "Pointfree functional composition / Forth style stack execution engine",
    "main": "./index.js",
    "typings": "./index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/thi-ng/umbrella.git"
    },
    "homepage": "https://github.com/thi-ng/umbrella/tree/master/packages/pointfree",
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "license": "Apache-2.0",
    "scripts": {
        "build": "yarn run clean && tsc --declaration",
        "clean": "rm -rf *.js *.d.ts .nyc_output build coverage doc",
        "cover": "yarn test && nyc report --reporter=lcov",
        "doc": "node_modules/.bin/typedoc --mode modules --out doc src",
        "pub": "yarn run build && yarn publish --access public",
        "test": "rm -rf build && tsc -p test && nyc mocha build/test/*.js"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.0",
        "mocha": "^5.2.0",
        "nyc": "^13.1.0",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.3"
    },
    "dependencies": {
        "@thi.ng/api": "^4.2.3",
        "@thi.ng/checks": "^1.5.13",
        "@thi.ng/equiv": "^0.1.14",
        "@thi.ng/errors": "^0.1.11"
    },
    "keywords": [
        "composition",
        "concatenative",
        "dataflow",
        "DSL",
        "ES6",
        "Forth",
        "functional",
        "pipeline",
        "pointfree",
        "stack",
        "data transformation",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    }
}
