{
	"name": "@thi.ng/interceptors",
	"version": "3.2.12",
	"description": "Interceptor based event bus, side effect & immutable state handling",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://github.com/thi-ng/umbrella/tree/develop/packages/interceptors#readme",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"contributors": [
		"Logan Powell (https://github.com/loganpowell)"
	],
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn clean && tsc --declaration",
		"clean": "rimraf --glob '*.js' '*.d.ts' '*.map' doc",
		"doc": "typedoc --excludePrivate --excludeInternal --out doc src/index.ts",
		"doc:ae": "mkdir -p .ae/doc .ae/temp && api-extractor run --local --verbose",
		"doc:readme": "yarn doc:stats && tools:readme",
		"doc:stats": "tools:module-stats",
		"pub": "yarn npm publish --access public",
		"test": "testament test"
	},
	"dependencies": {
		"@thi.ng/api": "^8.8.0",
		"@thi.ng/atom": "^5.2.1",
		"@thi.ng/checks": "^3.3.12",
		"@thi.ng/errors": "^2.2.15",
		"@thi.ng/logger": "^1.4.13",
		"@thi.ng/paths": "^5.1.36"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.34.4",
		"@thi.ng/testament": "^0.3.15",
		"rimraf": "^4.4.1",
		"tools": "workspace:^",
		"typedoc": "^0.23.28",
		"typescript": "^5.0.4"
	},
	"keywords": [
		"async",
		"browser",
		"event",
		"functional",
		"queue",
		"side-effect",
		"typescript"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./event-bus": {
			"default": "./event-bus.js"
		},
		"./interceptors": {
			"default": "./interceptors.js"
		}
	},
	"thi.ng": {
		"related": [
			"atom",
			"hdom",
			"rdom"
		],
		"status": "completed",
		"year": 2016
	}
}
