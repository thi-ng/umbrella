		"line",
		"morton",
		"random",
		"spiral",
		"typescript",
		"z-curve",
		"zigzag"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=12.7"
	},
	"files": [
		"./*.js",
		"./*.d.ts"
	],
	"exports": {
		".": {
			"default": "./index.js"
		},
		"./api": {
			"default": "./api.js"
		},
		"./circle": {
			"default": "./circle.js"
		},
