{
	"name": "@thi.ng/webgl",
	"version": "6.9.81",
	"description": "WebGL & GLSL abstraction layer",
	"type": "module",
	"module": "./index.js",
	"typings": "./index.d.ts",
	"sideEffects": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/thi-ng/umbrella.git"
	},
	"homepage": "https://thi.ng/webgl",
	"funding": [
		{
			"type": "github",
			"url": "https://github.com/sponsors/postspectacular"
		},
		{
			"type": "patreon",
			"url": "https://patreon.com/thing_umbrella"
		},
		{
			"type": "liberapay",
			"url": "https://liberapay.com/thi.ng"
		}
	],
	"author": "Karsten Schmidt (https://thi.ng)",
	"contributors": [
		"@nkint (https://github.com/nkint)",
		"@stwind (https://github.com/stwind)"
	],
	"license": "Apache-2.0",
	"scripts": {
		"build": "yarn build:esbuild && yarn build:decl",
		"build:decl": "tsc --declaration --emitDeclarationOnly",
		"build:esbuild": "esbuild --format=esm --platform=neutral --target=es2022 --tsconfig=tsconfig.json --outdir=. src/**/*.ts",
		"clean": "bun ../../tools/src/clean-package.ts geo shaders textures",
		"doc": "typedoc --options ../../typedoc.json --out doc src/index.ts",
		"doc:readme": "bun ../../tools/src/module-stats.ts && bun ../../tools/src/readme.ts",
		"pub": "yarn npm publish --access public",
		"test": "bun test",
		"tool:tangle": "../../node_modules/.bin/tangle src/**/*.ts"
	},
	"dependencies": {
		"@thi.ng/api": "^8.12.4",
		"@thi.ng/canvas": "^1.0.25",
		"@thi.ng/checks": "^3.7.20",
		"@thi.ng/equiv": "^2.1.94",
		"@thi.ng/errors": "^2.5.44",
		"@thi.ng/logger": "^3.2.3",
		"@thi.ng/matrices": "^3.0.23",
		"@thi.ng/memoize": "^4.0.28",
		"@thi.ng/object-utils": "^1.2.12",
		"@thi.ng/pixel": "^7.5.12",
		"@thi.ng/shader-ast": "^1.1.25",
		"@thi.ng/shader-ast-glsl": "^1.0.44",
		"@thi.ng/shader-ast-stdlib": "^1.0.44",
		"@thi.ng/transducers": "^9.6.12",
		"@thi.ng/vector-pools": "^3.2.73",
		"@thi.ng/vectors": "^8.6.8"
	},
	"devDependencies": {
		"esbuild": "^0.25.11",
		"typedoc": "^0.28.14",
		"typescript": "^5.9.3"
	},
	"keywords": [
		"2d",
		"3d",
		"animation",
		"blend",
		"browser",
		"buffer",
		"canvas",
		"datastructure",
		"declarative",
