		"math",
		"matrix",
		"noise",
		"random",
		"raymarch",
		"rgb",
		"sdf",
		"shader",
		"texture",
		"typescript",
		"webgl"
	],
	"publishConfig": {
		"access": "public"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"./*.js",
		"./*.d.ts",
		"color",
		"controlflow",
		"fog",
		"isec",
		"light",
		"math",
