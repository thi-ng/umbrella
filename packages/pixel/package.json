        "multiformat",
        "normal",
        "pixel",
        "pyramid",
        "resize",
        "rgb565",
        "sample",
        "sharpen",
        "typedarray",
        "typescript"
    ],
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=12.7"
    },
    "files": [
        "*.js",
        "*.d.ts",
        "format",
        "internal"
    ],
    "exports": {
